{
  "openapi": "3.0.0",
  "info": {
    "title": "My API",
    "version": "1.0.0"
  },
  "paths": {
    "/place/information": {
      "get": {
        "summary": "Get information about a place",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": {
                  "placeName": "Example Place",
                  "details": "Information about the place"
                }
              }
            }
          }
        }
      }
    },
    "/place/checkIn": {
      "post": {
        "summary": "Check in at a place",
        "responses": {
          "200": {
            "description": "Successful check-in",
            "content": {
              "application/json": {
                "example": {
                  "message": "Check-in successful",
                  "timestamp": "2023-12-24T12:00:00Z"
                }
              }
            }
          }
        }
      }
    }
  }
}
